@{
    ViewData["Title"] = "Home Page";
}
@model BuildsViewModel

<div class="row">
    <div class="col-md-3">
        <svg height="210" width="400">
            
            @foreach (var build in Model.Builds)
            {
                <path d="@build.Path" class="style_@build.Score">
                    <title>@build.Tooltip</title>    
                </path>

            }

        </svg>
    </div>
    <div class="col-md-3">
       
    </div>
    <div class="col-md-3">
       
    </div>
    <div class="col-md-3">
       
    </div>
</div>
